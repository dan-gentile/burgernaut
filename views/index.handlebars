

  <img class="logo" src="../assets/images/logo.png" alt="astronaut eating a burger" width="360px" height="360px">


{{!-- <h3>Burger Queue</h3> --}}
<div class="queue">
  
  <div class="queue-box">
  <ul>
    {{#each burger}}
      {{#unless eaten}}
        <li class="queue-item">
          <img src="../assets/images/burgerFull.png" alt="burger icon" width="38px" height="45px">
	        <h4>{{name}}</h4>
	        <button class="change-eaten" data-id="{{id}}" data-neweat="true">EAT ME</button>
        </li>
      {{/unless}}
    {{/each}}
  </ul>
  </div>

</div>


<div class="spent">
  
  <div class="spent-box">
    {{!-- <h3>Spent Fuel</h3> --}}
<ul>
   {{#each burger}}
    {{#if eaten}}
    <li class="spent-item">
      <img src="../assets/images/burgerEaten.png" alt="burger eaten icon" width="76px" height="90px">
	        <h4>{{name}}</h4>
          <i class="fad fa-dumpster delete" data-id="{{id}}"></i>
    </li>
    {{/if}}
  {{/each}}
</ul>
</div>
</div>

<div class="create">
  <div class="create-box">
<h3>Eat A Burger!</h3>
<form class="create-form">
  <div class="form-group">
    <input type="text" id="burger" name="name" placeholder="Burger">
  </div>
  <button id="submit" type="submit">Submit</button>
</form>
  </div>
</div>

