

<h2>Burger Queue</h2>

<ul>
  {{#each burger}}
    {{#unless eaten}}
    <li>
	  {{name}}
	    <button class="change-eaten" data-id="{{id}}" data-neweat="true">EAT ME</button>
    </li>
    {{/unless}}
  {{/each}}
</ul>

<h2>Spent Fuel</h2>

<ul>
   {{#each burger}}
    {{#if eaten}}
    <li>
	  {{name}}
    </li>
    {{/if}}
  {{/each}}
</ul>

<h2>Eat A Burger!</h2>
<form class="create-form">
  <div class="form-group">
    <label for="burger">Burger</label>
    <input type="text" id="burger" name="name">
  </div>
  <button type="submit">Submit</button>
</form>
